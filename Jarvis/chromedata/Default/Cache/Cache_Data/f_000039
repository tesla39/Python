                visits.get(m).add(l.get(\"sessionid\"));\n            }\n            if (l.get(\"ctl\").equals(\"products\")) {\n                String k = URLEncoder.encode(l.get(\"act\"), \"UTF-8\");\n                if (product.containsKey(k)) {\n                    product.put(k, product.get(k) + 1);\n                } else {\n                    product.put(k, 1);\n                }\n            } else if (l.get(\"ctl\").equals(\"event\")) {\n                if (l.get(\"act\").equals(\"add_cart\")) {\n                    line_data2[0] = line_data2[0] + 1;\n                } else if (Arrays.asList(\"checkout\", \"create_order\", \"buy_it_now\").contains(l.get(\"act\"))) {\n                    line_data2[1] = line_data2[1] + 1;\n                } else if (l.get(\"act\").equals(\"pay_success\")) {\n                    line_data2[2] = line_data2[2] + 1;\n                }\n            }\n        }\n\n        int vnum = vnums.values().stream().mapToInt(Integer::intValue).sum();\n        for (int i = 10; i >= 1; i--) {\n            if (visits.containsKey(i)) {\n                line_data.add(visits.get(i).size());\n            } else {\n                line_data.add(0);\n            }\n        }\n\n        List<Map<String, Object>> products = new ArrayList<>();\n        for (Map.Entry<String, Integer> entry : product.entrySet()) {\n            Map<String, Object> productItem = new HashMap<>();\n            productItem.put(\"url\", \"products/\" + URLDecoder.decode(entry.getKey(), \"UTF-8\"));\n            productItem.put(\"num\", entry.getValue());\n            products.add(productItem);\n        }\n\n        List<Map<String, Object>> dmaps = new ArrayList<>();\n        List<Map<String, Object>> countrys = db.merges(false).index(\"code\").findAll(\"addrs_countrys\", new HashMap<>(), \"id,name,zh_name,code\");\n        for (Map<String, Object> c : countrys) {\n            String k = (String) c.get(\"code\");\n            if (vnums2.containsKey(k)) {\n                Ma